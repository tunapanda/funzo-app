<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="org.tunapanda.funzoapp" version="1.0.6">
    <name>Certell eLibrary</name>
    <description>
        A reader for learning content distributed by Certell (certell.org)
    </description>
    <author email="dev@certell.org" href="http://certell.org">
        The Tunapanda Institute, on behalf of Certell.org
    </author>
    <content src="index.html"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <allow-navigation href="http://192.168.1.102:4200/*"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="xwalkVersion" value="19+"/>
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect"/>
    <preference name="xwalkMode" value="embedded"/>
    <preference name="xwalkMultipleApk" value="true"/>
    <preference name="AndroidPersistentFileLocation" value="Compatibility"/>
    <preference name="StatusBarBackgroundColor" value="#1E4A5D"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <plugin name="cordova-plugin-crosswalk-webview" spec="~1.8.0"/>
    <plugin name="cordova-plugin-background-download" spec="https://github.com/sgrebnov/cordova-plugin-background-download"/>
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3"/>
    <engine name="android" spec="~5.2.0"/>
    <plugin name="cordova-plugin-file-transfer" spec="~1.5.0"/>
    <engine name="ios" spec="latest"/>
    <plugin name="cordova-plugin-zip" spec="~3.1.0"/>
    <feature name="File">
        <param name="ios-package" value="CDVFile"/>
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar"/>
    </feature>
    <platform name="android">
        <icon height="24" src="icons/android/Icon-App-24x24.png" width="24"/>
        <icon height="36" src="icons/android/Icon-App-36x36.png" width="36"/>
        <icon height="48" src="icons/android/Icon-App-48x48.png" width="48"/>
        <icon height="72" src="icons/android/Icon-App-72x72.png" width="72"/>
    </platform>
    <platform name="ios">
